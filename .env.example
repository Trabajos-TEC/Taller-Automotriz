# Variables de entorno para Netlify + Neon
# IMPORTANTE: No commitear este archivo con valores reales

# Netlify Database (Neon) - URL con connection pooling
NETLIFY_DATABASE_URL=postgresql://neondb_owner:your_password@your-host.neon.tech/your_database?sslmode=require

# Netlify Database (Neon) - URL sin connection pooling (para migraciones)
NETLIFY_DATABASE_URL_UNPOOLED=postgresql://neondb_owner:your_password@your-host.neon.tech/your_database?sslmode=require

# Configuración del servidor (si usas el backend Express)
PORT=3001
NODE_ENV=development

# JWT Secret para autenticación
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# Netlify CLI (opcional)
NETLIFY_AUTH_TOKEN=
NETLIFY_SITE_ID=
